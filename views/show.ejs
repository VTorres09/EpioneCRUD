  
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Epione CRUD</title>
</head>
<body>

<h2>Registros</h2>
  <table border="1">
    <thead>
        <tr>
            <td>First Name</td>
            <td>Last Name</td>
            <td>Action</td>
        </tr>
    </thead>
    <tbody>
    <% data.forEach(function(details) { %>
        <tr>
            <td><%= details.first_name %></td>
            <td><%= details.last_name %></td>
            <td><a href="/edit/<%= details.id %>">Editar</a> - <a href="/delete/<%= details.id %>">Deletar</a></td>
        </tr>
        <% }) %>
    </tbody>
  </table>
  <button><a href="/">Novo</a></button>
</body>
</html>